<p id="notice"><%= notice %></p>

<h1>Listing Viatico Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @viatico_details.each do |viatico_detail| %>
      <tr>
        <td><%= link_to 'Show', viatico_detail %></td>
        <td><%= link_to 'Edit', edit_viatico_detail_path(viatico_detail) %></td>
        <td><%= link_to 'Destroy', viatico_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Viatico detail', new_viatico_detail_path %>
