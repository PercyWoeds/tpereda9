<p id="notice"><%= notice %></p>

<h1>Listing Tipo Unidads</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>User</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_unidads.each do |tipo_unidad| %>
      <tr>
        <td><%= tipo_unidad.code %></td>
        <td><%= tipo_unidad.name %></td>
        <td><%= tipo_unidad.user_id %></td>
        <td><%= tipo_unidad.company_id %></td>
        <td><%= link_to 'Show', tipo_unidad %></td>
        <td><%= link_to 'Edit', edit_tipo_unidad_path(tipo_unidad) %></td>
        <td><%= link_to 'Destroy', tipo_unidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo unidad', new_tipo_unidad_path %>
