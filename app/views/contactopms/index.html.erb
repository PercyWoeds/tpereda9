
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css">
<script
  src="https://code.jquery.com/jquery-3.1.1.min.js"
  integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
  crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>



<div class="ui form segment raised">

<p id="notice"><%= notice %></p>

<h1>Contactos - Proyectos Mineros </h1>

<table>
  <thead>
    <tr>
      <th>Proyecto minero</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactopms.each do |contactopm| %>
      <tr>
        <td><%= contactopm.proyecto_minero.descrip %></td>
        <td><%= link_to 'Ver', contactopm %></td>
        <td><%= link_to 'Editar', edit_contactopm_path(contactopm) %></td>

        <% if @current_user.level == "admin" %>
        <td><%= link_to 'Eliminar', contactopm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
         <% end %> 
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div class="extra content">


<%= link_to 'Nuevo', new_contactopm_path,class: "ui primary button"  %>


<%= link_to 'Exportar to PDF', "/contactopms/pdf/rpt_contactospm_pdf",class: "ui primary button"  %>  


<%= link_to 'Regresar', "/companies/examens02/1" ,class: "ui primary button"  %>
</div>

</div>
