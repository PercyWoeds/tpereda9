<p id="notice"><%= notice %></p>

<h1>Listing Autoviatics</h1>

<table>
  <thead>
    <tr>

      <th>Location</th>
      <th>Proyecto minerdo</th>
      <th>Employee</th>
      <th>Fecha</th>
      <th>Tema</th>
      <th>Supplier</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autoviatics.each do |autoviatic| %>
      <tr>
        <td><%= autoviatic.location_id %></td>
        <td><%= autoviatic.proyecto_minero_id %></td>
        <td><%= autoviatic.employee_id %></td>
        <td><%= autoviatic.fecha %></td>
        <td><%= autoviatic.tema %></td>
        <td><%= autoviatic.supplier_id %></td>
        
        <td><%= link_to 'Show', autoviatic %></td>
        <td><%= link_to 'Edit', edit_autoviatic_path(autoviatic) %></td>
        <td><%= link_to 'Destroy', autoviatic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

