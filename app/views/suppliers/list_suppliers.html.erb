<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css">
<script
  src="https://code.jquery.com/jquery-3.1.1.min.js"
  integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
  crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>



<div class="breadcrumbs">
  <%= link_to "My companies", "/companies" %> &raquo;
  <%= link_to @company.name, @company %>
</div>

<h1>
  <%= image_tag("supplier_48.png", :class => "vmiddle") %>
  Proveedores 
</h1>

<div class="small">
  <form method="get" action="/companies/suppliers/<%= @company.id %>">
    <strong>Search:</strong>
    <input type="text" name="search" value="<% if @q_org %><%= @q_org %><% end %>" class="small" style="width: 250px;" onclick="$(this).select();" />
    <input type="submit" value="Find" class="bold small_btn" />
    <a href="/companies/suppliers/<%= @company.id %>" class="small_btn">Clear</a>
  </form>
</div>
<br>

<div class="extra content">
  <%= link_to 'Nuevo proveedor', "/suppliers/new/#{@company.id}/#{@tipo}",class:"ui primary button" %>

  <%= link_to 'Carga desde Sunat ', "/suppliers/new2/#{@company.id}" ,class:"ui primary button"%>
 


</div>
<%= render "suppliers/list" %>

<br />


 
<%= will_paginate @suppliers %>




<h2>Importar Archivo proveedores "*.csv"</h2>

<%= form_tag import_suppliers_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Importar " %>
<% end %>


 Download:
 
  <%= link_to "CSV", suppliers_path(format: "csv"), multipart: true  %> 



<% if current_user.level == "examen" || current_user.level == "admin"%>
<div class="extra content">


  <%= link_to 'Reporte ', "/suppliers/pdf/#{@company.id}" ,class:"ui primary button"%>
 


</div>

<% end %>