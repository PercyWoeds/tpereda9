<p id="notice"><%= notice %></p>

<h1>Listado Conductores</h1>

<table>
  <thead>
    <tr>
      <th>Lugar</th>
      <th>Anio</th>
      <th>Licencia</th>
      <th>Categoria</th>
      <th>Expedicion licencia</th>
      <th>Revalidacion licencia</th>
      <th>Categoria especial</th>
      <th>Expedicion licencia especial</th>
      <th>Iqbf</th>
      <th>Fecha emision</th>
      <th>Dni emision</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conductors.each do |conductor| %>
      <tr>
        <td><%= conductor.lugar %></td>
        <td><%= conductor.anio %></td>
        <td><%= conductor.licencia %></td>
        <td><%= conductor.categoria %></td>
        <td><%= conductor.expedicion_licencia %></td>
        <td><%= conductor.revalidacion_licencia %></td>
        <td><%= conductor.categoria_especial %></td>
        <td><%= conductor.expedicion_licencia_especial %></td>
        <td><%= conductor.iqbf %></td>
        <td><%= conductor.fecha_emision %></td>
        <td><%= conductor.dni_emision %></td>
       
        <td><%= link_to 'Show', conductor %></td>
        <td><%= link_to 'Edit', edit_conductor_path(conductor) %></td>
        <td><%= link_to 'Destroy', conductor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conductor', new_conductor_path %>
