      <div class="small">
        <strong>Seleccionar ORDEN DE SERVICIO :</strong>
        <span id="selected_ost"><% if @ac_ost %><%= @ac_ost %><% else %><!-- i --><% end %></span>
      </div>
     
      

    

      <div class="small">
