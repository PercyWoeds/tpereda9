


<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css">
<script
  src="https://code.jquery.com/jquery-3.1.1.min.js"
  integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
  crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>


<p id="notice"><%= notice %></p>

<h1>Listing Exmautorizs</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Tramite </th>
      <th>Tipo Tramite</th>
      <th>Empleado </th>
      <th>Proyecto Minero </th>
     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exmautorizs.each do |exmautoriz| %>
      <tr>
         <td><%= exmautoriz.code %></td>

        <td><%= exmautoriz.tramit.name %></td>
        <td><%= exmautoriz.tipo_tramit.name %></td>
        <td><%= exmautoriz.employee.full_name%></td>
        <td><%= exmautoriz.proyecto_minero.descrip %></td>
        
        <td><%= link_to 'Ver', exmautoriz %></td>
        <td><%= link_to 'Editar', edit_exmautoriz_path(exmautoriz) %></td>
       <% if current_user.level =="admin"   %>
        <td><%= link_to 'Eliminar', exmautoriz, method: :delete, data: { confirm: 'Are you sure?' } %>
       <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

 <div class="form-actions">

<%= link_to 'Nuevo ', new_exmautoriz_path %>
</div>
