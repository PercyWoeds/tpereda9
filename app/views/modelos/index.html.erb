<p id="notice"><%= notice %></p>

<h1>Listing Modelos</h1>

<table>
  <thead>
    <tr>
      <th>Descrip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @modelos.each do |modelo| %>
      <tr>
        <td><%= modelo.descrip %></td>
        <td><%= link_to 'Show', modelo %></td>
        <td><%= link_to 'Edit', edit_modelo_path(modelo) %></td>
        <td><%= link_to 'Destroy', modelo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Modelo', new_modelo_path %>


<h2>Importar Archivo modelos "*.csv"</h2>

<%= form_tag import_modelos_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Importar " %>
<% end %>


 Download:
  <%= link_to "xls", modelos_path(format: "xls"), multipart: true  %> 
  