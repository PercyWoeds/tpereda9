<p id="notice"><%= notice %></p>

<h1>Listing Program Exms</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Ind</th>
      <th>Tr</th>
      <th>Em</th>
      <th>Lo</th>
      <th>Tema</th>
      <th>Fecha</th>
      <th>Inicio</th>
      <th>Termino</th>
      <th>Totalhora</th>
      <th>Horaingbase</th>
      <th>Observa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @program_exms.each do |program_exm| %>
      <tr>
        <td><%= program_exm.employee_id %></td>
        <td><%= program_exm.ind %></td>
        <td><%= program_exm.tr %></td>
        <td><%= program_exm.em %></td>
        <td><%= program_exm.lo %></td>
        <td><%= program_exm.tema %></td>
        <td><%= program_exm.fecha %></td>
        <td><%= program_exm.inicio %></td>
        <td><%= program_exm.termino %></td>
        <td><%= program_exm.totalhora %></td>
        <td><%= program_exm.horaingbase %></td>
        <td><%= program_exm.observa %></td>
        <td><%= link_to 'Show', program_exm %></td>
        <td><%= link_to 'Edit', edit_program_exm_path(program_exm) %></td>
        <td><%= link_to 'Destroy', program_exm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

